class Solution {
  public:
    // Function to return the count of number of elements in union of two arrays.
    int findUnion(vector<int>& a, vector<int>& b) {
        // code here
        
        unordered_map<int,int> mp;
        
        for(int i=0;i<a.size();i++){
            mp[a[i]];
        }
        
        for(int i=0;i<b.size();i++){
            mp[b[i]];
        }
        
        return mp.size();
        
    }
};
